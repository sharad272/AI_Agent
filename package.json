{
    "name": "jarvis-code-assistant",
    "displayName": "Jarvis Code Assistant",
    "description": "AI-powered code assistant with streaming responses",
    "version": "0.1.0",
    "engines": {
        "vscode": "^1.60.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:aiagent.openJarvis"
    ],
    "main": "./extension.js",
    "contributes": {
        "commands": [
            {
                "command": "aiagent.openJarvis",
                "title": "Open Jarvis Code Assistant"
            }
        ],
        "keybindings": [
            {
                "command": "aiagent.openJarvis",
                "key": "ctrl+shift+a",
                "mac": "cmd+shift+a"
            }
        ]
    },
    "scripts": {
        "lint": "eslint ."
    },
    "dependencies": {
        "node-fetch": "^2.6.1"
    },
    "devDependencies": {
        "@types/vscode": "^1.60.0",
        "eslint": "^7.27.0"
    }
}
